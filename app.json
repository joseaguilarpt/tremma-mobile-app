{
  "expo": {
    "name": "tremma-mobile-app",
    "slug": "tremma-mobile-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/tremma-lg.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "splash": {
      "image": "./src/assets/images/tremma-lg.png",
      "imageWidth": 300,
      "resizeMode": "contain",
      "backgroundColor": "#1f223a"
    },
    "android": {
      "jsEngine": "hermes",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/android-icon-96x96.png",
        "backgroundColor": "#1e1847"
      },
      "package": "com.joseaguilarpt.tremmamobileapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/android-icon-96x96.png"
    },
    "plugins": [
      "expo-router",
      "expo-splash-screen",
      [
        "expo-font",
        {
          "assets": ["./src/assets/fonts/"]
        }
      ],
      "expo-build-properties"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "cec49e41-ff33-4c34-9d46-c2af936142d7"
      }
    },
    "owner": "joseaguilarpt"
  }
}
