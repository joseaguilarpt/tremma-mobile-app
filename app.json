{
  "expo": {
    "name": "tremma-mobile-app",
    "slug": "tremma-mobile-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/tremma-lg.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "splash": {
      "image": "./assets/images/tremma-lg.png",
      "imageWidth": 300,
      "resizeMode": "contain",
      "backgroundColor": "#1f223a"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon-96x96.png",
        "backgroundColor": "#1e1847"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/android-icon-96x96.png"
    },
    "assets": ["./assets/fonts/"],
    "plugins": ["expo-router", "expo-splash-screen"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
